<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="t_customer" catalog="dog"
		class="com.dog.model.Customer">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="t_dog" catalog="dog" class="com.dog.model.Dog">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="t_order" catalog="dog" class="com.dog.model.Order">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="t_ddog" catalog="dog" class="com.dog.model.Ddog">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="barrage" catalog="dog" class="com.dog.model.Barrage">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="comments" catalog="dog" class="com.dog.model.Comments">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>
